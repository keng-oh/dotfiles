# 日本語を使用
export LANG=ja_JP.UTF-8

# add PATH
export PATH="$HOME/bin:$PATH"

# add Alias
source ~/.alias

# use emacs keybind
bindkey -e

# setting zsh-completions
if [ -e /usr/local/share/zsh-completions ]; then
  fpath=(/usr/local/share/zsh-completions $fpath)
fi

# zsh history
HISTFILE=$HOME/.zsh_history
HISTSIZE=100000
SAVEHIST=1000000

# options
source ~/.zsh/.zshrc.options

# prompt
source ~/.zsh/.zshrc.prompt

# complement
source ~/.zsh/.zshrc.complement
